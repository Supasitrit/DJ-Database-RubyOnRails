<p id="notice"><%= notice %></p>

<h1>Listing Geartypes</h1>

<table class="table">
  <thead>
    <tr>
      <th colspan="3">name</th>
    </tr>
  </thead>

  <tbody>
    <% @geartypes.each do |geartype| %>
      <tr>
        <td><%= link_to geartype.name, geartype %></td>
        <td><%= link_to 'Show', geartype %></td>
        <td><%= link_to 'Edit', edit_geartype_path(geartype) %></td>
        <td><%= link_to 'Destroy', geartype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Geartype', new_geartype_path %>
