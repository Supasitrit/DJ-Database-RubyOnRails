<p id="notice"><%= notice %></p>

<h1>Listing Djs</h1>

<table>
  <thead>
    <tr>
      <th>Genre</th>
      <th>Text</th>
      <th>Id</th>
      <th>Integer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @djs.each do |dj| %>
      <tr>
        <td><%= dj.genre %></td>
        <td><%= dj.text %></td>
        <td><%= dj.id %></td>
        <td><%= dj.integer %></td>
        <td><%= link_to 'Show', dj %></td>
        <td><%= link_to 'Edit', edit_dj_path(dj) %></td>
        <td><%= link_to 'Destroy', dj, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dj', new_dj_path %>
